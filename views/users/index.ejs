<%- include('../partials/header') %>
<div class="container">
    <div class="row">
        <div class="col s12">
            <div class="card">
                <div class="card-content">
                    <div>
                        <ul>
                            <% posts.forEach(function(p) { %>
                                <li>
                                    <a href="/posts/<%= p.id %>"><%= p.title %></a>
                                </li>
                            <% }); %>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include('../partials/footer') %>